<!-- Extensions for running and debugging ips tests -->
   <extension
         point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            label="%org.faktorips.devtools.core.launchIpsTest"
            icon="icons/TestCaseRun.gif"
            helpContextId=""
            class="org.faktorips.devtools.core.ui.IpsTestLaunchShortcut"
            modes="run, debug"
            id="org.faktorips.devtools.core.launchIpsTest">
         <contextualLaunch>
            <enablement>
               <with variable="selection">
               		<iterate>
                 		<or>
                   			<test property="org.eclipse.debug.ui.matchesPattern" value="*.ipstestcase"/>
                   			<test property="org.eclipse.debug.ui.matchesPattern" value="*.ipsproduct"/>
                   			<adapt type="org.eclipse.core.resources.IResource"/>
                 		</or>
               		</iterate>
               </with>
            </enablement>
         </contextualLaunch>
         <perspective
               id="org.eclipse.jdt.ui.JavaPerspective">
         </perspective>
         <perspective
               id="org.eclipse.jdt.ui.JavaHierarchyPerspective">
         </perspective>
         <perspective
               id="org.eclipse.jdt.ui.JavaBrowsingPerspective">
         </perspective>
         <perspective
               id="org.eclipse.debug.ui.DebugPerspective">
         </perspective>
      </shortcut>
   </extension>
