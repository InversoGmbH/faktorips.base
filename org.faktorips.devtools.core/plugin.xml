   <extension
         point="org.eclipse.ui.commands">
      <command
            name="%org.faktorips.devtools.core.openIpsObject.label"
            description="%org.faktorips.devtools.core.openIpsObject.tooltip"
            categoryId="org.faktorips.devtools.core.ui.category.navigate"
            id="org.faktorips.devtools.core.openIpsObject">
      </command>
      <category
            id="org.faktorips.devtools.core.ui.category.navigate"
            name="%org.faktorips.devtools.core.ui.category.navigate"/>
      <command
            name="%org.faktorips.devtools.core.launchIpsTest.run"
            description="%org.faktorips.devtools.core.launchIpsTest.run.description"
            categoryId="org.eclipse.debug.ui.category.run"
            id="org.faktorips.devtools.core.launchIpsTest.run">
      </command>
      <command
            name="%org.faktorips.devtools.core.launchIpsTest.debug"
            description="%org.faktorips.devtools.core.launchIpsTest.debug.description"
            categoryId="org.eclipse.debug.ui.category.run"
            id="org.faktorips.devtools.core.launchIpsTest.debug">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <!--  win32:  M1=CTRL,    M2=SHIFT, M3=ALT, M4=-
            carbon: M1=COMMAND, M2=SHIFT, M3=ALT, M4=CTRL -->
      <key
            sequence="M1+M2+I"
            commandId="org.faktorips.devtools.core.openIpsObject"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            sequence="M3+M2+D I"
            commandId="org.faktorips.devtools.core.launchIpsTest.debug"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            sequence="M3+M2+X I"
            commandId="org.faktorips.devtools.core.launchIpsTest.run"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>            
   </extension>
   

<!-- 
    Extensions to the Java perspective
  -->
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <actionSet id="org.faktorips.devtools.actions.StandardActionSet"/>
         <actionSet
               id="org.faktorips.devtools.actions.ModelingActionSet">
         </actionSet>
         <actionSet
               id="org.faktorips.devtools.actions.ProductDefinitionActionSet">
         </actionSet>
         <view
               relative="org.eclipse.jdt.ui.PackageExplorer"
               visible="false"
               relationship="stack"
               id="org.faktorips.plugin.pdexplorer">
         </view>
         <view
               closeable="true"
               id="org.faktorips.devtools.core.ui.views.testRunner"
               moveable="true"
               relationship="stack"
               relative="org.eclipse.jdt.ui.PackageExplorer"
               showTitle="true"
               standalone="false"
               visible="false"/>
         <view
               closeable="true"
               id="org.faktorips.devtools.core.ui.views.modelExplorer"
               moveable="true"
               relationship="stack"
               relative="org.eclipse.jdt.ui.PackageExplorer"
               showTitle="true"
               standalone="false"
               visible="true"/>
         <actionSet id="org.faktorips.devtools.actions.NavigationActionSet"/>
         <view
               closeable="true"
               id="org.faktorips.devtools.core.ui.views.modelDescription"
               moveable="true"
               ratio="0.75"
               relationship="stack"
               relative="org.eclipse.ui.views.ContentOutline"
               showTitle="true"
               standalone="false"
               visible="true"/>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension targetID="org.faktorips.devtools.core.productDefinitionPerspective">
         <view
               closeable="false"
               id="org.faktorips.devtools.core.ui.views.productDefinitionExplorer"
               moveable="true"
               ratio="0.25"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               standalone="false"
               visible="true"/>
         <view
               closeable="false"
               id="org.faktorips.devtools.core.ui.views.productStructureExplorer"
               moveable="true"
               ratio="0.5"
               relationship="bottom"
               relative="org.faktorips.devtools.core.ui.views.productDefinitionExplorer"
               showTitle="true"
               standalone="false"
               visible="true"/>
         <view
               closeable="false"
               id="org.eclipse.ui.views.ProblemView"
               moveable="true"
               ratio="0.75"
               relationship="bottom"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               standalone="false"
               visible="true"/>
         <actionSet id="org.faktorips.devtools.actions.StandardActionSet"/>
         <actionSet id="org.faktorips.devtools.actions.ProductDefinitionActionSet"/>
         <view
               closeable="true"
               id="org.faktorips.devtools.core.ui.views.modelExplorer"
               moveable="true"
               relationship="stack"
               relative="org.faktorips.devtools.core.ui.views.productDefinitionExplorer"
               showTitle="true"
               standalone="false"
               visible="false"/>
         <view
               closeable="true"
               id="org.faktorips.devtools.core.ui.views.testRunner"
               moveable="true"
               relationship="stack"
               relative="org.faktorips.devtools.core.ui.views.productDefinitionExplorer"
               showTitle="true"
               standalone="false"
               visible="false"/>
         <actionSet id="org.faktorips.devtools.actions.NavigationActionSet"/>
         <view
               closeable="true"
               id="org.faktorips.devtools.core.ui.views.modelDescription"
               moveable="true"
               ratio="0.8"
               relationship="right"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               standalone="false"
               visible="true"/>
      </perspectiveExtension>
   </extension>
   <extension point="org.eclipse.ui.popupMenus">  
		<objectContribution adaptable="true" id="addIpsNature" objectClass="org.eclipse.jdt.core.IJavaProject">
            <menu id="faktorIPSMenu" label="%faktorIPSMenu" path="additions">
			    <separator name="faktorIpsGroup"/>
            </menu>
			<action
		    class="org.faktorips.devtools.core.ui.actions.AddIpsNatureAction"
		    enablesFor="*"
		    icon="icons/AddIpsNature.gif"
		    id="addIpsNatureAction"
		    label="%addIpsNatureAction"
		    menubarPath="faktorIPSMenu/faktorIpsGroup"/>
		  		<!-- <filter name="nature" value="org.eclipse.emf.codegen.jet.IJETNature"/> -->
		</objectContribution>
    	<objectContribution id="fixDifferencesAction" objectClass="org.eclipse.core.runtime.IAdaptable">
            <menu id="faktorIPSMenu" label="%faktorIPSMenu" path="additions">
			    <separator name="faktorIpsGroup"/>
            </menu>
	    	<action
            class="org.faktorips.devtools.core.ui.wizards.fixdifferences.OpenFixDifferencesToModelWizardAction"
            enablesFor="*"
            icon="icons/FixDifferencesToModel.gif"
            id="fixDifferencesAction"
            label="%fixDifferencesAction"
            menubarPath="faktorIPSMenu/faktorIpsGroup">
	            <enablement>
		             <or>
			              <objectClass name="org.eclipse.jdt.core.IJavaProject"/>
			              <objectClass name="org.eclipse.core.resources.IResource"/>
			              <objectClass name="org.faktorips.devtools.core.model.ipsobject.IFixDifferencesToModelSupport"/>
		             </or>
	            </enablement> 
            </action>
		      <visibility>
	             <or>
		              <objectClass name="org.eclipse.jdt.core.IJavaProject"/>
		              <objectClass name="org.eclipse.core.resources.IResource"/>
		              <objectClass name="org.faktorips.devtools.core.model.ipsobject.IFixDifferencesToModelSupport"/>
	             </or>
		      </visibility> 
    	</objectContribution>
        <objectContribution id="migrateIpsProject"
            objectClass="org.eclipse.jdt.core.IJavaProject">
            <menu id="faktorIPSMenu" label="%faktorIPSMenu" path="additions">
			    <separator name="faktorIpsGroup"/>
            </menu>            
            <action
            class="org.faktorips.devtools.core.ui.wizards.migration.OpenMigrationWizardAction"
            enablesFor="*"
            icon="icons/MigrationWizard.gif"
            id="migrateIpsProject"
            label="%org.faktorips.devtools.core.Migrate.label"
            menubarPath="faktorIPSMenu/faktorIpsGroup"
            tooltip="%org.faktorips.devtools.core.Migrate.tooltip"/>
        </objectContribution>

  </extension>
  
